        select(name="status").select
          option(value="" disabled selected) Estado civil
          option(value="Single") Soltero
          option(value="Married") Casado
          option(value="Married") Viudo
          option(value="Married") Divorciado
        select(name="dependants").select
          option(value="" disabled selected) Número de hijos
          option(value="0") Ninguno
          option(value="1") 1
          option(value="2") 2
          option(value="3") 3
